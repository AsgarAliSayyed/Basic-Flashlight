        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:text="Group Member Name:"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:text="Lukesh Ingale"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:text="Sarthak Gaikwad"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:text="Asgar Ali Sayyed"/>
</LinearLayout>
</LinearLayout>
